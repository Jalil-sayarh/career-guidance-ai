{
  "name": "neo4j-bloom",
  "version": "2.11.0",
  "description": "Neo4j Bloom is a business productivity application for interactive exploration of graph data.",
  "icons": [
    {
      "src": "Bloom_Icon_BlueBG.svg",
      "type": "svg"
    }
  ],
  "files": [
    "dist",
    "*.md",
    "LICENSES.txt",
    "NOTICE.txt",
    "signature.pem",
    "Bloom_Icon_BlueBG.svg"
  ],
  "main": "dist/index.html",
  "neo4jDesktop": {
    "apiVersion": "^1.4.0",
    "licenseRequired": false
  },
  "browserslist": [
    "last 3 version"
  ],
  "scripts": {
    "lint": "tsc && yarn lint:prettier && yarn lint:eslint",
    "lint:prettier": "prettier 'src/**/*.{js,jsx,ts,tsx}' 'playwright/**/*.{js,jsx,ts,tsx}' --write",
    "lint:eslint": "eslint --quiet --fix 'src/**/*.{js,jsx,ts,tsx}' 'playwright/**/*.{js,mjs,jsx,ts,tsx}'",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test /playwright/pullrequest/  --project=chromium --config playwright/playwright.config.ts",
    "e2e:sso": "playwright test /playwright/sso/  --project=chromium --config playwright/playwright.config.ts",
    "e2e:plugin": "playwright test /playwright/plugin/  --project=chromium --config playwright/playwright.config.ts",
    "e2e:license": "playwright test /playwright/license/  --project=chromium --config playwright/playwright.config.ts",
    "e2e:apoc": "playwright test /playwright/apoc/  --project=chromium --config playwright/playwright.config.ts",
    "e2e:debug": "playwright test --headed --project=chromium --config playwright/playwright.config.ts",
    "e2e:gds": "playwright test /playwright/gds/ --project=chromium --config playwright/playwright.config.ts",
    "e2e:inspector": "PWDEBUG=1 playwright test --project=chromium --config playwright/playwright.config.ts",
    "e2e:install": "playwright install --with-deps",
    "bundle": "cd dist && ../node_modules/.bin/bestzip ../$npm_package_name-$npm_package_version-assets.zip * && cd ..",
    "postinstall": "yarn generate-dependencies",
    "generate-dependencies": "node scripts/generate-license-lists.js",
    "dev": "vite",
    "devSSL": "PERSISTSTORE=\"true\" HTTPS=\"true\" vite",
    "devNoChecks": "NO_CHECKS=\"true\" vite",
    "devClean": "PERSISTSTORE=\"false\"  vite",
    "build": "tsc && vite build",
    "build:release": "yarn generate-dependencies && rm -rf ./dist && NODE_OPTIONS=--max_old_space_size=8192 SETUP_SENTRY=true SETUP_SEGMENT=true vite build",
    "build:nx": "sh scripts/insightToNx.sh",
    "preview": "vite preview",
    "previewSSL": "HTTPS=\"true\" vite preview",
    "prepare": "echo hi"
  },
